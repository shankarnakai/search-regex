{
    "manifest_version": 2,
    "name": "Search Regex",
    "description": "search with regex",
    "version": "0.1",
    "icons": {
        "128": "128.png"
    },
    "content_scripts": [{
	"matches": [ "<all_urls>" ],
        "js": ["mark.js", "search.js"]
    }],
    "permissions": [
        "unlimitedStorage",
        "notifications"
    ]
}
